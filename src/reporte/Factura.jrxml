	<queryString>
		<![CDATA[SELECT
     factura."idfactura" AS factura_idfactura,
     factura."categoria" AS factura_categoria,
     factura."idcarril" AS factura_idcarril,
     factura."cedula" AS factura_cedula,
     factura."fecha" AS factura_fecha,
     factura."hora" AS factura_hora,
     factura."pago" AS factura_pago,
     factura."devuelta" AS factura_devuelta,
     factura."placa" AS factura_placa,
     estacion."nombre" AS estacion_nombre,
     estacion."ciudad" AS estacion_ciudad,
     categoria."categoria" AS categoria_categoria,
     categoria."precio" AS categoria_precio,
     empleado."cedula" AS empleado_cedula,
     empleado."correo" AS empleado_correo,
     empleado."nombre" AS empleado_nombre,
     empleado."apellidos" AS empleado_apellidos,
     empleado."cargo" AS empleado_cargo,
     empleado."contrasenia" AS empleado_contrasenia,
     empleado."direccion" AS empleado_direccion,
     empleado."estado" AS empleado_estado,
     empleado."telefono" AS empleado_telefono,
     carril."idcarril" AS carril_idcarril,
     carril."nombre" AS carril_nombre,
     carril."sentido" AS carril_sentido,
     carril."estado" AS carril_estado
FROM
     "public"."categoria" categoria INNER JOIN "public"."factura" factura ON categoria."categoria" = factura."categoria"
     INNER JOIN "public"."empleado" empleado ON factura."cedula" = empleado."cedula"
     INNER JOIN "public"."carril" carril ON factura."idcarril" = carril."idcarril"
     INNER JOIN "public"."estacion" estacion ON carril."nombre" = estacion."nombre"

where factura."idfactura"= $P{idFac}]]>
	</queryString>
